{
  "manifest_version": 2,

  "name": "Fake Speed Test",
  "description": "Launch Fake Speed Test",
  "version": "1.0.0",
  "content_scripts" : [
    {
      "matches": ["https://*/*"],
      "js": ["popup.js"]
    }
  ],
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab","tabs","<all_urls>","declarativeContent"]
}
